{
  "name": "demo3d",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "autocrlf": "git config core.autocrlf false",
    "preview": "vite preview --host",
    "mirror:get": "npm config get registry",
    "mirror:set": "npm config set registry https://registry.npmmirror.com",
    "mirror:rm": "npm config rm registry",
    "git:ignorecase": "git config core.ignorecase false",
    "dev": "vite --mode dev --host",
    "dev:test": "vite --mode test --host",
    "dev:prod": "vite --mode prod --host",
    "build:dev": "vue-tsc --noEmit && vite --mode dev build",
    "build:test": "vue-tsc --noEmit && vite --mode test build",
    "build": "vite --mode prod build",
    "git:gitee": "git add . && git commit -m \"自动备份\" && git push gitee",
    "git:github": "git add . && git commit -m \"自动备份\" && git push github",
    "git:all": "git add . && git commit -m \"自动备份\" && git push gitee && git push github"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "axios": "^1.7.2",
    "cannon-es": "^0.20.0",
    "dat.gui": "^0.7.9",
    "dayjs": "^1.11.10",
    "element-plus": "^2.7.3",
    "lodash": "^4.17.21",
    "mitt": "^3.0.1",
    "mqtt": "^5.2.1",
    "nipplejs": "^0.10.2",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "three": "^0.165.0",
    "three-mesh-bvh": "^0.7.4",
    "vue": "^3.4.27",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@types/dat.gui": "^0.7.13",
    "@types/lodash": "^4.17.4",
    "@types/node": "^20.12.12",
    "@types/three": "^0.164.1",
    "@vitejs/plugin-vue": "^5.0.4",
    "fast-glob": "^3.3.2",
    "gsap": "^3.12.5",
    "sass": "^1.77.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.12",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-mock": "2.9.8",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-tsc": "^2.0.19",
    "vite-plugin-glsl": "^1.3.0"
  }
}
